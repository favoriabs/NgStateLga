{
    "name": "favoriabs/ngstatelga",
    "description": "A laravel package that helps with Nigerian states and local government areas",
    "keywords": ["laravel", "Nigeria", "States", "Local Government", "Lgas"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Favor Oriabure",
            "email": "favoriabs@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
      "php": ">=5.4.0",
      "Illuminate/support": "~5"
    },
     "autoload": {
        "psr-4": {
            "Favoriabs\\NgStateLga\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Favoriabs\\NgStateLga\\NgStateLgaServiceProvider"
            ],
            "aliases": {
                "NgStateLga": "Favoriabs\\NgStateLga\\Facade"
            }
        }
    },
}
